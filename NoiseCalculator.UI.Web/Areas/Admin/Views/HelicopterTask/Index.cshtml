@model NoiseCalculator.UI.Web.Areas.Admin.Models.HelicopterTask.HelicopterTaskIndexViewModel

@{
    //ViewBag.Title = "Index";
    ViewBag.Title = @Model.PageTitle;
    Layout = "~/Areas/Admin/Views/Shared/AdminIndex.cshtml";
}
@section AdminHidden
{
    <input id="urlCreate" type="hidden" value="@Model.UrlCreate" />
    <input id="urlEdit" type="hidden" value="@Model.UrlEdit" />
    <input id="urlDeleteConformation" type="hidden" value="@Model.UrlDeleteConfirmation" />
}
@section PageTitleText
{
    @Model.PageTitle
}
@section ItemList
{
    @Html.Partial("_HelicopterTaskItemListTable", Model.HelicopterTasks)
}
