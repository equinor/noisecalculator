@using NoiseCalculator.UI.Web.Areas.Admin.Resources
@model NoiseCalculator.UI.Web.Areas.Admin.Models.Generic.GenericDefinitionIndexViewModel

@{
    ViewBag.Title = AdminResources.PageTitleNoiseProtection;
    Layout = "~/Areas/Admin/Views/Shared/AdminIndex.cshtml";
}

@section AdminHidden
{
    <input id="urlCreate" type="hidden" value="@Url.Action("Create")" />
    <input id="urlEdit" type="hidden" value="@Url.Action("Edit")" />
    <input id="urlDeleteConformation" type="hidden" value="@Url.Action("ConfirmDelete")" />
}
@section PageTitleText
{
    @AdminResources.PageTitleNoiseProtection
}
@section ItemList
{
    @Html.Partial("_GenericItemListTable", Model.Definitions)
}
